{
  "name": "prisma-effect-kysely",
  "version": "4.5.0",
  "description": "Prisma generator that creates Effect Schema types from Prisma schema compatible with Kysely",
  "license": "MIT",
  "author": "Samuel Ho",
  "main": "dist/generator/index.js",
  "bin": {
    "prisma-effect-kysely": "dist/generator/index.js"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/generator/index.d.ts",
      "import": "./dist/generator/index.js"
    },
    "./kysely": {
      "types": "./dist/kysely/helpers.d.ts",
      "import": "./dist/kysely/helpers.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "SECURITY.md",
    "CODE_OF_CONDUCT.md",
    "CONTRIBUTING.md"
  ],
  "sideEffects": false,
  "keywords": [
    "prisma",
    "generator",
    "effect",
    "schema",
    "typescript",
    "type-safety",
    "prisma-generator",
    "effect-ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samuelho-dev/prisma-effect-kysely.git"
  },
  "bugs": {
    "url": "https://github.com/samuelho-dev/prisma-effect-kysely/issues"
  },
  "homepage": "https://github.com/samuelho-dev/prisma-effect-kysely#readme",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@10.0.0",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "prepare": "husky",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit -p tsconfig.lib.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "changeset": "changeset",
    "changeset:version": "changeset version",
    "changeset:publish": "changeset publish",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run test && npm run build"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@prisma/generator-helper": "^7.2.0",
    "prettier": "^3.7.4"
  },
  "peerDependencies": {
    "effect": "^3.19.13",
    "kysely": "^0.28.9"
  },
  "peerDependenciesMeta": {
    "kysely": {
      "optional": true
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@changesets/changelog-github": "^0.5.1",
    "@effect/vitest": "^0.27.0",
    "@eslint/js": "^9.39.2",
    "@prisma/internals": "^7.2.0",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/eslint-plugin": "^1.5.4",
    "effect": "^3.19.13",
    "eslint": "^9.39.2",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.7",
    "kysely": "^0.28.9",
    "lint-staged": "^16.2.7",
    "prisma": "^7.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.0",
    "vitest": "^4.0.16"
  }
}
